{
    "env": {
      "browser": true,
      "es6": true,
      "commonjs": true
    },
    "extends": [
      "eslint:recommended", 
      "plugin:react/recommended", 
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 9,
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": ["react"],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "react/prop-types": 0,
      "react/jsx-uses-vars": [2],
      "no-console": 0
    }
  }